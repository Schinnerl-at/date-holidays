{
  "name": "date-holidays",
  "version": "0.2.0-0",
  "description": "worldwide holidays",
  "main": "src",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/*.mocha.js",
    "pack": "npm run yaml && npm run lint && npm test && npm pack",
    "yaml": "node scripts/yaml2json.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- -R dot --check-leaks test/*.mocha.js",
    "lint": "eslint --cache --quiet '**/*.js'",
    "watch": "watch-run -p data/*.yaml npm run yaml & babel --watch -d lib src",
    "transpile": "babel -d lib src",
    "clean": "rimraf coverage node_modules"
  },
  "dependencies": {
    "caldate": "^1.0.0",
    "date-easter": "^0.2.2",
    "lodash": "^4.11.1",
    "moment-timezone": "^0.5.3",
    "moonbeams": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "hashtree": "^0.7.0",
    "istanbul": "^0.4.3",
    "js-yaml": "^3.5.5",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "serialize-to-js": "^0.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/date-holidays.git"
  },
  "keywords": [
    "holidays"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/commenthol/date-holidays/issues"
  },
  "homepage": "https://github.com/commenthol/date-holidays",
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  }
}
